          },
          "type": "array"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "ManagedFieldsEntry": {
      "properties": {
        "manager": {
          "type": "string"
        },
        "operation": {
          "type": "string"
        },
        "apiVersion": {
          "type": "string"
        },
        "time": {
          "type": "string",
          "format": "data-time"
        },
        "fieldsType": {
          "type": "string"
        },
        "fieldsV1": {
          "$schema": "http://json-schema.org/draft-04/schema#",
