        "kube_service_refs": {
          "$schema": "http://json-schema.org/draft-04/schema#",
          "$ref": "#/definitions/DestinationSelector_KubeServiceRefs"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "DestinationSelector_KubeServiceMatcher": {
      "properties": {
        "labels": {
          "patternProperties": {
            ".*": {
              "type": "string"
            }
          },
          "type": "object"
        },
        "namespaces": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "clusters": {
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "DestinationSelector_KubeServiceRefs": {
