{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "$ref": "#/definitions/AccessPolicy",
  "definitions": {
    "AccessPolicy": {
      "properties": {
        "kind": {
          "type": "string"
        },
        "apiVersion": {
          "type": "string"
        },
        "metadata": {
          "$schema": "http://json-schema.org/draft-04/schema#",
          "$ref": "#/definitions/ObjectMeta"
        },
        "spec": {
          "$schema": "http://json-schema.org/draft-04/schema#",
          "$ref": "#/definitions/AccessPolicySpec"
        },
        "status": {
          "$schema": "http://json-schema.org/draft-04/schema#",
          "$ref": "#/definitions/AccessPolicyStatus"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "AccessPolicySpec": {
      "properties": {
        "source_selector": {
          "items": {
            "$schema": "http://json-schema.org/draft-04/schema#",
            "$ref": "#/definitions/IdentitySelector"
          },
          "type": "array"
        },
        "destination_selector": {
          "items": {
            "$schema": "http://json-schema.org/draft-04/schema#",
            "$ref": "#/definitions/DestinationSelector"
          },
          "type": "array"
        },
        "allowed_paths": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "allowed_methods": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "allowed_ports": {
          "items": {
            "type": "integer"
          },
          "type": "array"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "AccessPolicyStatus": {
      "properties": {
        "observed_generation": {
          "type": "integer"
        },
        "state": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "integer"
            }
          ]
        },
        "destinations": {
          "patternProperties": {
            ".*": {
              "$schema": "http://json-schema.org/draft-04/schema#",
              "$ref": "#/definitions/ApprovalStatus"
            }
          },
          "type": "object"
        },
        "workloads": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "errors": {
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "ApprovalStatus": {
      "properties": {
        "acceptance_order": {
          "type": "integer"
        },
        "state": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "integer"
            }
          ]
        },
        "errors": {
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "ClusterObjectRef": {
      "properties": {
        "name": {
          "type": "string"
        },
        "namespace": {
          "type": "string"
        },
        "cluster_name": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object"
