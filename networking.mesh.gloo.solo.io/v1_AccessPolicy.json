      "additionalProperties": false,
      "type": "object"
    },
    "IdentitySelector_KubeIdentityMatcher": {
      "properties": {
        "namespaces": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "clusters": {
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "IdentitySelector_KubeServiceAccountRefs": {
      "properties": {
